                                                        aria-label="Select example"
                                                        v-model="dataUsuario.idTipoDocumento">
                                                        <option v-for="option in datosGenerales.tipoDocumento"
                                                            v-bind:value="option.ID_TIPO_DOCUMENTO">{{ option.NOMBRE }}
                                                        </option>
                                                    </select>
                                                </div>



                                                <div class="fv-row col-md-12 mt-5">
                                                    <div class="form-group">
                                                        <label class="required form-label">Número de Documento</label>
                                                        <div class="input-group">
                                                            <input name="numDocumento" ref="numDocumento"
