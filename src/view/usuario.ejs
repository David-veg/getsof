                                                                        <!--end::Visibility toggle-->
                                                                    </div>
                                                                    <!--end::Input wrapper-->

                                                                    <!--begin::Highlight meter-->
                                                                    <div class="d-flex align-items-center mb-3"
                                                                        data-kt-password-meter-control="highlight">
                                                                        <div
                                                                            class="flex-grow-1 bg-secondary bg-active-primary rounded h-5px me-2">
                                                                        </div>
                                                                        <div
                                                                            class="flex-grow-1 bg-secondary bg-active-primary rounded h-5px me-2">
                                                                        </div>
                                                                        <div
                                                                            class="flex-grow-1 bg-secondary bg-active-primary rounded h-5px me-2">
                                                                        </div>
                                                                        <div
