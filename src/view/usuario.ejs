                                                                <!-- {{dataSucursales}} -->
                                                            </div>
                                                            <div class="table-responsive">
                                                                <table class="table table-hover">
                                                                    <thead class="bg-primary text-white fw-bolder">
                                                                        <tr>
                                                                            <th width="5%">#</th>
                                                                            <th width="20%">RUC</th>
                                                                            <th width="60%">Empresa</th>
                                                                            <th width="15%">Acción</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr v-for="(item, i) in datosGenerales.arrayEmpresasAgregadas">
                                                                            <td>{{i+1}}</td>
                                                                            <td>{{item.ruc}}</td>
                                                                            <td>{{item.empresa}}</td>
                                                                            <td><button type="button" class="btn btn-sm btn-primary btn-hover-scale me-5" v-on:click="quitarEmpresa(i)">Quitar</button></td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12 text-center pt-10">

                                            <button type="submit"
                                                class="btn btn-bg-success text-inverse-success me-2 mb-2"
                                                v-if="!dataUsuario.idUsuario">{{dom.nombreBotonRegistrar}}</button>
                                            <button type="submit"
